<script src="http://ajax.aspnetcdn.com/ajax/jquery.validate/1.11.1/jquery.validate.min.js"></script>
<script type="text/javascript">
    jQuery.validator.addMethod("equal", function(value, element, param) {
      return this.optional(element) || value == param;
    }, "Captcha is invalid. Please Enter valid captcha");
    $(document).ready( function(){
    $(window).resize(function(){
      resizeScreen();
    });
    resizeScreen();
    $(".contact-form").validate({
      rules: {
        "contact_message[name]": { required: true},
        "contact_message[email]": { required: true, email: true },
        "contact_message[message]": { required: true },
        "contact_message[phone]": { required: true, number: true },
        "contact_message[captcha]": { required: true, equal: "28" }
      }
    });
  });
  function resizeScreen(){
    $("#videoframe").height($("#videoframe").width()/1.78);
  }
    var stack = $('.stack').paperstack({});
    $('#next').click(stack.next);
    $('#prev').click(stack.previous);
    var owl = $('.loop');
    owl.owlCarousel({
        center: true,
        nav: true,
        items: 1,
        loop: true,
        margin: 30,
        autoplay: true,
        navText: ["<img src=assets/testimonial/test_slide_arrow.png>", "<img src=assets/testimonial/test_slide_arrow.png>"],
        responsive: {
            767: {
                items: 5,
            },
            576:{
                items: 3,
            },
            300:{
                items: 1
            }
        }
    });

    var detail_block = document.getElementsByClassName('details')[0];
    // console.log(detail_block);
    var author_name = document.querySelector('.center').querySelector(".item").getAttribute("data-author");
    var slide_det = document.querySelector('.center').querySelector(".item").querySelector("p").innerHTML;
    detail_block.querySelector(".dynamic-data").querySelector("span").innerHTML = author_name;
    detail_block.querySelector(".dynamic-data p").innerHTML = slide_det;

    // Listen to owl events:
    owl.on('changed.owl.carousel', function (event) {
        detail_block.querySelector(".dynamic-data").classList.remove("animated", "fadeIn");
        setTimeout(function () {
            console.log(event.target.querySelector('.center').querySelector(".item").getAttribute("data-author"));
            var detail_block = document.getElementsByClassName('details')[0];
            // console.log(detail_block);
            var author_name = event.target.querySelector('.center').querySelector(".item").getAttribute("data-author");
            var slide_det = event.target.querySelector('.center').querySelector(".item").querySelector("p").innerHTML;
            detail_block.querySelector(".dynamic-data").classList.add('animated', 'fadeIn');
            detail_block.querySelector(".dynamic-data").querySelector("span").innerHTML = author_name;
            detail_block.querySelector(".dynamic-data p").innerHTML = slide_det;

        }, 10);
    });
</script>